 100 FOR Y = 0 TO 191
 110   GOSUB 1000
 120   FOR X = 0 TO 39
 130     POKE BASE + X,Y
 140 NEXT X,Y
 150 FOR Y = 128 TO 191
 160   GOSUB 1000
 170   FOR X = 40 TO 47
 180     POKE BASE + X,Y + 64
 190 NEXT X,Y
 200 END

1000 L = Y : S = 0 : Q = 0
1010 S = INT(L / 64)
1020 L = L - S * 64
1030 Q = INT(L / 8)
1040 L = L - Q * 8
1050 BASE = 8192 + 1024 * L + 128 * Q + 40 * S
1060 RETURN
